{
  "problemMatcher": [
    {
      "owner": "pylint-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]+): ([^:]+):(\\d+):(\\d+): (E\\d+): (.*)\\((.*)\\)$",
          "file": 2,
          "line": 3,
          "column": 4,
          "code": 7,
          "message": 6
        }
      ]
    },
    {
      "owner": "pylint-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^:]+): ([^:]+):(\\d+):(\\d+): (W\\d+): (.*)\\((.*)\\)$",
          "file": 2,
          "line": 3,
          "column": 4,
          "code": 7,
          "message": 6
        }
      ]
    }
  ]
}
